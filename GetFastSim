local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "BaconBoss Hub | Get Fast Simulator", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

local TpTab = Window:MakeTab({
	Name = "Main",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Tp4Tab = Window:MakeTab({
	Name = "Others",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

_G.win = true 
function win()
     while _G.win == true do
        local tweenspeed = 50000
        local Players = game:GetService("Players")
                local TweenService = game:GetService("TweenService")
                local Workspace = game:GetService("Workspace")
                local LocalPlayer = Players.LocalPlayer
        local function TweenTo(cf, dur)
                    local Character = LocalPlayer.Character
                    if not Character or not Character:FindFirstChild("HumanoidRootPart") then
                        return
                    end
        
                    local rootPart = Character:WaitForChild("HumanoidRootPart")
                    local direction = (cf.p - rootPart.Position).unit
                    local targetCFrame = CFrame.new(cf.p)
                    local distance = (targetCFrame.p - rootPart.CFrame.p).Magnitude
                    local tweenInfo = TweenInfo.new(distance / tweenspeed, Enum.EasingStyle.Linear)
                    local tween = TweenService:Create(rootPart, tweenInfo, {
                        CFrame = targetCFrame
                    })
                    tween:Play()
                    tween.Completed:Wait() 
                    return tween
                end
        local BV = Instance.new('BodyVelocity', game.Workspace)
                        BV.Parent = game.Players.LocalPlayer.Character.HumanoidRootPart
                        BV.velocity = Vector3.new(0, 0, 0)
                        BV.maxForce = Vector3.new(9e9, 9e9, 9e9)
                        BV.Name = "/ugcleaks"
        TweenTo(CFrame.new(-5112.5, 10, 21550, 1, 0, 0, 0, 1, 0, 0, 0, 1))      
        wait(1)
        game:GetService("Players").LocalPlayer.Character.Humanoid.Health = 0
        wait(2.5)
    end
end

TpTab:AddButton({
	Name = "Inf Yeild",
	Callback = function()
        loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
    end
}) 

TpTab:AddToggle({
    Name = "Auto Best Win Farm",
    Default = false,
    Callback = function(Value)
        _G.win = Value
        win()
    end    
})

_G.click = true
function click()
     while _G.click == true do
        game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("ClickForSpeed"):InvokeServer()
     wait(0)
    end
end

TpTab:AddToggle({
    Name = "Auto Speed",
    Default = false,
    Callback = function(Value)
        _G.click = Value
        click()
    end    
})

_G.rb = true
function rb()
      while _G.rb == true do
        game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Rebirth"):FireServer()
     wait(0.5)
   end
end

TpTab:AddToggle({
    Name = "Auto Rebirth",
    Default = false,
    Callback = function(Value)
        _G.rb = Value
        rb()
    end    
})

_G.egg = true
function egg()
     while _G.egg == true do
        local args = {
            [1] = "LegendaryEgg"
        }
        
        game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("BuyPetEgg"):InvokeServer(unpack(args))
      wait(0)
    end
end

TpTab:AddToggle({
    Name = "Auto Hatch Godly Pets",
    Default = false,
    Callback = function(Value)
        _G.egg = Value
        egg()
    end    
})

Tp4Tab:AddButton({
    Name = "Anti AFK",
    Callback = function()
        local vu = game:GetService("VirtualUser")
        game:GetService("Players").LocalPlayer.Idled:connect(function()
            vu:Button2Down(Vector2.new(0, 0), workspace.CurrentCamera.CFrame)
            wait(1)
            vu:Button2Up(Vector2.new(0, 0), workspace.CurrentCamera.CFrame)
        end)
    end
})

Tp4Tab:AddButton({
    Name = "Delete Time Hold Button E",
    Callback = function()
        while true do
            for _, v in next, workspace:GetDescendants() do
                if v:IsA("ProximityPrompt") then
                    v.HoldDuration = 0
                end
            end
            wait(1) 
        end
    end
})

Tp4Tab:AddButton({
    Name = "Server Less People",
    Callback = function()
        local Http = game:GetService("HttpService")
        local TPS = game:GetService("TeleportService")
        local Api = "https://games.roblox.com/v1/games/"

        local _place = game.PlaceId
        local _servers = Api.._place.."/servers/Public?sortOrder=Asc&limit=100"

        local function ListServers(cursor)
            local Raw = game:HttpGet(_servers .. ((cursor and "&cursor="..cursor) or ""))
            return Http:JSONDecode(Raw)
        end

        local Server, Next
        repeat
            local Servers = ListServers(Next)
            Server = Servers.data[1]
            Next = Servers.nextPageCursor
        until Server

        TPS:TeleportToPlaceInstance(_place, Server.id, game.Players.LocalPlayer)
    end
})

Tp4Tab:AddButton({
    Name = "Server Hop",
    Callback = function()
        local module = loadstring(game:HttpGet("https://raw.githubusercontent.com/LeoKholYt/roblox/main/lk_serverhop.lua"))()
        module:Teleport(game.PlaceId)
    end
})

Tp4Tab:AddSlider({
	Name = "Change Speed Power",
	Min = 16,
	Max = 1000,
	Default = 5,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Speed",
	Callback = function(Value)
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
	end    
})

Tp4Tab:AddSlider({
	Name = "Change Jump Power",
	Min = 16,
	Max = 1000,
	Default = 5,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Jump",
	Callback = function(Value)
		game.Players.LocalPlayer.Character.Humanoid.JumpPower = Value
	end    
})
